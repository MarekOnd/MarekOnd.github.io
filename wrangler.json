{
  "name": "bloghugo",
  "compatibility_date": "2025-11-01",
  "assets": {
    "directory": "./public",
    "html_handling": "auto-trailing-slash",
    "not_found_handling": "404-page",
    "run_worker_first": false
  },
  "build": {
    "command": "hugo build"
  },
  "workers_dev": true,
  "preview_urls": true
}